# local path where to store MongoDB
MONGODB_PATH=./mongodb
# MongoDB port
MONGODB_PORT=27017

# Mongo Express port
MONGO_EXPRESS_PORT=8081
# Mongo Express username
MONGO_EXPRESS_USERNAME=username
# Mongo Express password
MONGO_EXPRESS_PASSWORD=password
# Mongo Express cookie secret
MONGO_EXPRESS_SITE_COOKIESECRET=cookiesecret
# Mongo Express session secret
MONGO_EXPRESS_SITE_SESSIONSECRET=sessionsecret
